?-  listlength([],0).
true.

?- listlength([],1).
false.

?- listlength([],N).
N = 0.

?- listlength([1,2,3],0).
false.

?- listlength([1,2,3],3).
true.

?- listlength([1,2,3],N).
N = 3.

?- listlength(L,3).
L = [_508, _514, _520] .

?- listlength(L,N).
L = [],
N = 0 ;
L = [_36],
N = 1 ;
L = [_36, _42],
N = 2 ;
L = [_36, _42, _48],
N = 3 ;
L = [_36, _42, _48, _54],
N = 4 ;
L = [_36, _42, _48, _54, _60],
N = 5 ;
L = [_36, _42, _48, _54, _60, _66],
N = 6 ;
L = [_36, _42, _48, _54, _60, _66, _72],
N = 7 .
